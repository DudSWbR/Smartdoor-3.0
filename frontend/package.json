{
  "name": "smartdoor",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/runtime": "7.0.0-beta.55",
    "@material-ui/core": "^4.5.0",
    "@material-ui/styles": "^4.5.0",
    "@react-pdf/renderer": "^1.6.12",
    "axios": "^0.19.0",
    "babel-plugin-root-import": "^6.4.1",
    "basic-auth": "^2.0.1",
    "customize-cra": "^0.8.0",
    "date-fns": "^2.0.0-beta.5",
    "formik": "^2.0.8",
    "framer-motion": "^1.6.15",
    "history": "^4.10.1",
    "husky": "^3.0.9",
    "immer": "^4.0.2",
    "lottie-web": "^5.6.4",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "query-string": "^6.10.1",
    "react": "^16.10.2",
    "react-app-rewired": "^2.1.3",
    "react-credit-cards": "^0.8.0",
    "react-dom": "^16.10.2",
    "react-dropzone": "10.1.9",
    "react-icons": "^3.7.0",
    "react-paginate": "^6.3.0",
    "react-player": "^1.15.2",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "react-scroll": "^1.7.14",
    "react-select": "^3.0.8",
    "react-svg": "^10.0.22",
    "react-text-mask": "^5.4.3",
    "react-toastify": "^5.4.0",
    "reactotron-react-js": "^3.3.4",
    "reactotron-redux": "^3.1.2",
    "reactotron-redux-saga": "^4.2.2",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.1",
    "source-map-explorer": "^2.1.0",
    "styled-components": "^4.4.0",
    "text-mask-addons": "^3.8.0",
    "yup": "^0.28.0"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "build:prod": "build",
    "test": "cross-env NODE_ICU_DATA=node_modules/full-icu react-app-rewired test",
    "test:watch": "react-app-rewired test --watchAll",
    "eject": "react-scripts eject",
    "coverage": "cross-env NODE_ICU_DATA=node_modules/full-icu react-app-rewired test --coverage --watchAll=false",
    "storybook": "start-storybook",
    "commit": "git-cz",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect",
      "jest-localstorage-mock"
    ],
    "moduleNameMapper": {
      "^~/(.*)": "<rootDir>/src/$1"
    },
    "collectCoverageFrom": [
      "!src/services/api.js",
      "!src/index.js",
      "!src/components/**/*.js",
      "!src/styles/**/*.js",
      "!src/pages/**/*.js",
      "!src/coverage/**/*.js",
      "!src/config/**/*.js",
      "!src/store/*.js",
      "!src/App.js"
    ],
    "coverageDirectory": "src/coverage"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "prepare-commit-msg": "cross-env exec < /dev/tty && git cz --hook || true",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@hot-loader/react-dom": "^16.10.2",
    "@storybook/addon-centered": "^5.2.6",
    "@storybook/addon-info": "^5.2.6",
    "@storybook/react": "^5.2.3",
    "@testing-library/jest-dom": "^4.1.2",
    "@testing-library/react": "^9.3.0",
    "@testing-library/react-hooks": "^2.0.3",
    "@types/jest": "^24.0.18",
    "axios-mock-adapter": "^1.17.0",
    "babel-jest": "^24.9.0",
    "cross-env": "^6.0.3",
    "cz-conventional-changelog": "3.0.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "full-icu": "^1.3.0",
    "jest-localstorage-mock": "^2.4.0",
    "lint-staged": "^10.0.0-1",
    "prettier": "^1.18.2",
    "react-app-rewire-hot-loader": "^2.0.1",
    "react-hot-loader": "^4.12.15",
    "react-test-renderer": "^16.10.2",
    "redux-mock-store": "^1.5.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
